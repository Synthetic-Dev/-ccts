{
	"$schema": "https://raw.githubusercontent.com/Synthetic-Dev/TypeScriptToLua/master/tsconfig-schema.json",
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"moduleResolution": "node",
		"strict": false,
		"declaration": true,
		"types": ["@jackmacwindows/lua-types/cc", "@ccts/craftos-types"]
	},
	"tstl": {
		"buildMode": "library",
		"luaTarget": "CC",
		"luaLibImport": "inline"
	},
	"include": ["index.ts"]
}
